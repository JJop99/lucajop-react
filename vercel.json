{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "api/works.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/works/[workId].js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/works",
      "dest": "/api/works.js"
    },
    {
      "src": "/api/works/(.*)",
      "dest": "/api/works/[workId].js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
    
  ]
}
